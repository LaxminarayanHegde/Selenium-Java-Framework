<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression Test Suite">
	<groups>
		<run>
			<include name = "Regression"/>
		</run>
	</groups>
	
	<listeners>
		<listener class-name="WebAutomationTests.TestComponents.Listeners" />
	</listeners>
	
  <test thread-count="5" name="Login Test"> 
    <classes>
      <class name="WebAutomationTests.tests.NegativeTests"/>
      <class name="WebAutomationTests.tests.StandAloneTest"/>
    </classes>
  </test>
  	
</suite> <!-- Suite -->
